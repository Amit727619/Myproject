                    </div>
                </nav> 

                <h1 style="text-align: center">Book Photographer</h1>
                <form action="/savex" method="post">
                    <div>
                        <label>Name : </label>
                        <input type="text" id="name" name="name" />
                        <label>Mobile No :</label>
                        <input type="text" id="mobno" name="mobno" />
                        <label>Email :</label>
                        <input type="text" id="email" name="email" />
                        <label>Address :</label>
                        <input type="text" id="address" name="address" />
                        <label>Photograph :</label>
                        <input type="text" id="amount" name="amount" />
                        <label>Date :</label>
                        <input type="date" id="date" name="date" />
                        <div class="button" style="height: 40px; width: 50px">
                            <button style="height: 49px; width: 70px" type="submit"><b>Submit </b></button>
                        </div>
                    </div>
                </form>
          </section>
        <script>
            let sidebar = document.querySelector(".sidebar");
            let sidebarBtn = document.querySelector(".sidebarBtn");
            sidebarBtn.onclick = function () {
                sidebar.classList.toggle("active");
                if (sidebar.classList.contains("active")) {
                    sidebarBtn.classList.replace("bx-menu", "bx-menu-alt-right");
                } else sidebarBtn.classList.replace("bx-menu-alt-right", "bx-menu");
            };
        </script>

